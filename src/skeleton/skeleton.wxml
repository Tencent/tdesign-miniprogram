<wxs src="./skeleton.wxs" module="this" />

<view wx:if="{{ loading }}" class="{{classPrefix}} {{prefix}}-class ">
  <view wx:if="{{ parsedRowcols.length }}" class="{{classPrefix}}__content">
    <view wx:for="{{parsedRowcols}}" wx:for-index="index" wx:for-item="row" wx:key="index" class="{{classPrefix}}__row">
      <view
        wx:for="{{ row }}"
        wx:for-index="index"
        wx:for-item="col"
        wx:key="index"
        class="{{col.class}} {{prefix}}-class-col"
        style="{{this.getStyle(col.style)}}"
      ></view>
    </view>
  </view>
</view>
<view wx:else class="{{classPrefix}}__content">
  <slot />
</view>
