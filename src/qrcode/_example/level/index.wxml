<radio-group class="horizontal-radio-group" bindchange="handleLevelChange">
  <label wx:for="{{levels}}" wx:key="value" class="radio-label">
    <radio value="{{item.value}}" checked="{{item.checked}}" />
    {{item.label}}
  </label>
</radio-group>

<t-qrcode value="https://tdesign.gtimg.com/site/tdesign-logo.png" level="{{currentLevel}}" status="active" />
