<view style="{{ customStyle }}" class="{{ prefix }}-class {{classPrefix}}">
  <view wx:if="{{label}}" class="{{classPrefix}}__label {{prefix}}-class-label">{{label}}</view>
  <view
    class="{{classPrefix}}__body {{prefix}}-class-body {{isActive ? classPrefix + '__body--active' : ''}} {{disabled ? classPrefix + '__body--disabled' : ''}}"
    style="{{ bodyStyle }}"
    bindtap="onTapBackground"
  >
    <view
      class="{{classPrefix}}__dot {{prefix}}-class-dot  {{isActive ? classPrefix + '__dot--active' : ''}}"
      catchtap="onTapDot"
    />
  </view>
</view>
