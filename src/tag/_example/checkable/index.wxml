      <text>{{item}}</text>
      <t-check-tag size="large" variant="{{item}}" content="{{ ['已选中态', '未选中态'] }}" />
      <t-check-tag default-checked size="large" variant="{{item}}" content="{{ ['已选中态', '未选中态'] }}" />
    </view>
  </block>
</view>
