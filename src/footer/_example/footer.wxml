<view class="demo-title">Footer 页脚</view>
<view class="demo-desc">用于基础列表展示，可附带文字、品牌logo、操作，常用商详、个人中心、设置等页面。</view>
<t-demo title="01 类型" desc="基础页脚">
  <!-- theme 为 text，只有版权信息 -->
  <t-footer theme="text" copyright="{{copyright}}" class="t-footer-demo" />
</t-demo>
<t-demo desc="基础加连接页脚">
  <!-- theme 为 text，含有底部链接 -->
  <t-footer theme="text" copyright="{{copyright}}" textLinkList="{{textLinkListOne}}" class="footer" />
  <!-- theme 为 text，含有两条底部链接 -->
  <view class="t-footer-demo--divider"></view>
  <t-footer theme="text" copyright="{{copyright}}" textLinkList="{{textLinkListTwo}}" class="footer" />
</t-demo>
<t-demo desc="基础加连接页脚">
  <!-- theme 为 logo -->
  <t-footer theme="logo" logo="{{logo}}" class="t-footer-demo" />
</t-demo>
