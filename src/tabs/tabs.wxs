/* eslint-disable */

function animate(options) {
  var result = [];

  result.push('--td-tab-content-transition-duration: ' + options.duration + 's');
  result.push(
    options.direction === 'Y'
      ? ';transform: translate3d( 0,' + -100 * options.currentIndex + '%, 0)'
      : ';transform: translate3d( ' + -100 * options.currentIndex + '%,0, 0)',
  );

  return result.join(';');
}

module.exports = {
  animate: animate,
};
