<template name="icon">
  <t-icon
    style="{{style || ''}}"
    class="{{class}}"
    t-class="{{tClass}}"
    prefix="{{prefix || ''}}"
    name="{{name || ''}}"
    size="{{size || 'inherit'}}"
    color="{{color || 'inherit'}}"
    aria-hidden="{{ariaHidden || '' }}"
    aria-label="{{ariaLabel || ''}}"
    aria-role="{{ariaRole || ''}}"
    bind:click="{{bindclick || ''}}"
  />
</template>
