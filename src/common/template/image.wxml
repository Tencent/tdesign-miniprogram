<template name="image">
  <t-image
    style="{{style}}"
    error="{{error || 'default'}}"
    lazy="{{lazy}}"
    loading="{{count || 'default'}}"
    shape="{{shape || 'square'}}"
    src="{{src}}"
    mode="{{mode || 'scaleToFill'}}"
    webp="{{webp}}"
    showMenuByLongpress="{{showMenuByLongpress}}"
    bind:error="{{binderror}}"
    bind:load="{{bindload}}"
    t-class="{{class}} {{tClass}}"
    t-class-load="{{tClassLoad}}"
  />
</template>
