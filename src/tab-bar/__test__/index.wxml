<t-tab-bar id="base" style="{{style}}" customStyle="{{customStyle}}" value="{{value}}" bindchange="onChange">
  <t-tab-bar-item
    wx:for="{{list}}"
    wx:key="index"
    id="item{{index+1}}"
    style="{{style}}"
    customStyle="{{customStyle}}"
    value="{{item.value}}"
    icon="{{item.icon}}"
    sub-tab-bar="{{item.children}}"
  >
    {{item.label}}
  </t-tab-bar-item>
</t-tab-bar>
