.demo__list {
  display: flex;
  flex-wrap: wrap;
  padding: 16rpx 32rpx;
}

.demo__card {
  flex: 0 0 25%;
  text-align: center;
}

.demo__card-name {
  font-size: 24rpx;
  color: #999;
}

/* 自定义图标 */
@font-face {
  font-family: 'icon'; /* Project id 3144196 */
  src: url('data:application/x-font-woff2;charset=utf-8;base64,')
      format('woff2'),
    url('//at.alicdn.com/t/font_3144196_s14ifjx2cyi.woff?t=1642299317916') format('woff'),
    url('//at.alicdn.com/t/font_3144196_s14ifjx2cyi.ttf?t=1642299317916') format('truetype');
}

.icon {
  font-family: 'icon' !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-a-0:before {
  content: '\e64d';
}

.icon-a-1h:before {
  content: '\e64e';
}

.icon-a-2h:before {
  content: '\e64f';
}

.icon-a-3h:before {
  content: '\e650';
}
