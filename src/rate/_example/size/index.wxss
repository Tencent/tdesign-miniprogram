.demo-rate {
  background-color: var(--bg-color-demo);
  color: var(--td-text-color-primary);
  height: 96rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 32rpx;
  border-bottom: solid 1rpx var(--bg-color-demo-border);
  border-top: solid 1rpx var(--bg-color-demo-border);
  margin-top: 32rpx;
  margin-bottom: 32rpx;
}

.demo-rate__title {
  width: 200rpx;
}

.demo-rate__transparent {
  background-color: transparent;
  padding-left: 32rpx;
  border: 0;
}
