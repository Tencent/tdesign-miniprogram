<wxs src="../calendar.wxs" module="this" />

<t-calendar
  visible="{{visible}}"
  value="{{value}}"
  switch-mode="year-month"
  minDate="{{minDate}}"
  maxDate="{{maxDate}}"
  bind:confirm="handleConfirm"
  bind:panel-change="handlePanelChange"
/>
<t-cell arrow title="带翻页功能的日历" note="{{this.formatTimestamp(value)}}" bind:tap="handleCalendar" />
