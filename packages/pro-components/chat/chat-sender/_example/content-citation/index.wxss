.demo-content-citation-container {
  padding: 56rpx 0 0 0;
  background-color: #ffffff;
  height: 568rpx;
  position: relative;
}

.demo-content-citation-footer{
  height: 32rpx;
  width: 100%;
  text-align: center;
  font-size: 20rpx;
  line-height: 32rpx;
  color: rgba(0, 0, 0, 0.4);
  position: absolute;
  bottom: 32rpx;
}


/* 聊天发送器包装器 */

.chat-sender-wrapper {
  /* border: 2rpx solid #e5e5e5; */
  border-radius: 8rpx;
  overflow: hidden;
}

.demo-footer-prefix{

  display: flex;
  align-items: center;
}

.deep-think-block{
  padding: 0 24rpx;
  height: 60rpx;
  border-radius: 200rpx;
  border: 2rpx solid rgba(220, 220, 220, 1);
  display: flex;
  justify-content: center;
  align-items: center;
  color: rgba(0, 0, 0, 0.9);
  margin-right: 16rpx;
}

.deep-think-text{
  margin-left: 8rpx;
}

.net-search-block{
  width: 64rpx;
  height: 60rpx;
  border: 2rpx solid rgba(220, 220, 220, 1);
  border-radius: 200rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.active{
  border-color: rgba(217, 225, 255, 1);
  color: rgba(0, 82, 217, 1);
  background-color: rgba(242, 243, 255, 1);
}

.demo-header{
  display: flex;
  width: calc(100% - 48rpx);
  margin-left: 24rpx;
  height: 62rpx;
  border-bottom: 2rpx solid rgba(231, 231, 231, 1);
}

.header-content{
  padding: 0 24rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #00000066;
}
