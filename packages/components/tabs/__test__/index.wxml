<t-tabs
  id="base"
  style="{{style}}"
  customStyle="{{customStyle}}"
  defaultValue="{{0}}"
  placement="{{placement}}"
  swipeable="{{swipeable}}"
  sticky
  stickyProps="{{stickyProps}}"
>
  <t-tab-panel id="first" style="{{style}}" customStyle="{{customStyle}}" label="标签页一" value="{{0}}"
    >标签一内容</t-tab-panel
  >
  <t-tab-panel wx:for="{{items}}" wx:key="index" label="{{ 'label' + item}}" value="{{item}}"
    >标签{{item}}内容</t-tab-panel
  >
</t-tabs>
