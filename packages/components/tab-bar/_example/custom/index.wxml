<view class="wrapper">
  <t-tab-bar t-class="t-tab-bar" value="{{value}}" fixed="{{false}}" bindchange="onChange">
    <t-tab-bar-item
      wx:for="{{list}}"
      wx:key="value"
      value="{{item.value}}"
      icon="{{item.icon}}"
      ariaLabel="{{item.ariaLabel}}"
    ></t-tab-bar-item>
  </t-tab-bar>
</view>
