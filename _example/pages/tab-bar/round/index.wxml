<t-tab-bar
  t-class="t-tab-bar"
  value="{{value}}"
  shape="round"
  theme="tag"
  fixed="{{false}}"
  split="{{false}}"
  bindchange="onChange"
>
  <t-tab-bar-item
    wx:for="{{list}}"
    wx:key="value"
    value="{{item.value}}"
    icon="{{item.icon}}"
    ariaLabel="{{item.ariaLabel}}"
  >
  </t-tab-bar-item>
</t-tab-bar>
