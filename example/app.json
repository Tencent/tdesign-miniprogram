  "subpackages": [
    {
      "root": "pages/side-bar/",
      "pages": ["side-bar", "base/index", "switch/index", "custom/index", "with-icon/index"]
    },
    {
      "root": "pages/action-sheet/",
      "pages": ["action-sheet"]
    },
    {
      "root": "pages/avatar/",
      "pages": ["avatar", "skyline/avatar"]
    },
    {
      "root": "pages/calendar/",
      "pages": ["calendar"]
    },
    {
      "root": "pages/dialog/",
      "pages": ["dialog", "skyline/dialog"]
    },
    {
      "root": "pages/picker/",
      "pages": ["picker", "skyline/picker"]
    },
    {
