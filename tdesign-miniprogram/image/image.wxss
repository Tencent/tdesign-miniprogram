.t-float-left {
  float: left;
}
.t-float-right {
  float: right;
}
@keyframes tdesign-fade-out {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}
.t-image__mask {
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #f2f2f2;
  color: #ccc;
}
.t-image--loading-text {
  width: 0px;
  height: 0px;
}
.t-image__common {
  width: 100%;
  height: 100%;
}
