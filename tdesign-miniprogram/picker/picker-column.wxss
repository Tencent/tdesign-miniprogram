.t-float-left {
  float: left;
}
.t-float-right {
  float: right;
}
@keyframes tdesign-fade-out {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}
t-picker-column {
  display: flex;
  flex: 1;
}
.t-picker-column__group {
  height: 400rpx;
  overflow: hidden;
  flex: 1;
}
.t-picker-column__wrapper {
  padding: 160rpx 0;
}
.t-picker-column__item {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 80rpx;
  line-height: 80rpx;
  color: #000000;
}
